{
  "name": "seed",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "apollo-client": "1.9.3",
    "apollo-link": "1.0.6",
    "apollo-link-http": "1.3.1",
    "apollo-server-express": "1.2.0",
    "body-parser": "1.18.2",
    "compression": "1.7.1",
    "config": "1.28.1",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "cross-env": "5.1.1",
    "es6-shim": "0.35.3",
    "express": "4.16.2",
    "graphql": "0.11.7",
    "graphql-tools": "2.11.0",
    "history": "4.7.2",
    "immutable": "4.0.0-rc.3",
    "jss-nested": "6.0.1",
    "jwt-decode": "2.2.0",
    "material-ui": "1.0.0-beta.21",
    "material-ui-icons": "1.0.0-beta.17",
    "morgan": "1.9.0",
    "node-fetch": "1.7.3",
    "react": "16.2.0",
    "react-apollo": "2.0.4",
    "react-cookie": "2.1.2",
    "react-dom": "16.2.0",
    "react-jss": "8.1.0",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.8",
    "react-scripts": "1.0.17",
    "recompose": "0.26.0",
    "redux": "3.7.2",
    "redux-actions": "2.2.1",
    "redux-form": "7.2.0",
    "redux-form-material-ui": "5.0.0-beta.1",
    "redux-immutable": "4.0.0",
    "redux-logger": "3.0.6",
    "styled-jss": "2.1.2"
  },
  "devDependencies": {
    "babel-eslint": "8.0.3",
    "concurrently": "3.5.1",
    "empty": "0.10.1",
    "enzyme": "3.2.0",
    "enzyme-to-json": "3.2.2",
    "eslint-config-fravega": "1.4.0",
    "flow-bin": "0.60.1",
    "flow-typed": "2.2.3",
    "jest-enzyme": "4.0.1",
    "modclean": "2.1.2",
    "source-map-explorer": "1.5.0"
  },
  "proxy": {
    "/api/*": {
      "target": "http://localhost:3001/"
    }
  },
  "scripts": {
    "analyze": "npm run build && source-map-explorer build/static/js/main.*",
    "lint": "eslint src && flow check",
    "flow": "flow",
    "update-types": "flow-typed update",
    "dev": "concurrently \"node server\" \"npm run client\"",
    "dev:debug": "concurrently \"npm run server:debug\" \"npm run client\"",
    "start": "cross-env NODE_ENV=production node server",
    "server:debug": "node --inspect-brk server",
    "client": "BROWSER=none react-scripts start",
    "build": "modclean -r && react-scripts build",
    "test": "react-scripts test --env=jsdom"
  }
}
